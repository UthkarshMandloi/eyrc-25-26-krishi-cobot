<sdf version="1.6">
    <model name="warehouse_divider">
	   <static>true</static>
      <link name="warehouse_divider_link">

        <visual name="visual">
          <geometry>
            <mesh><uri>model://warehouse_divider/meshes/warehouse_divider.dae</uri></mesh>
          </geometry>
        </visual>

        <collision name="collision">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
          <box>
            <size>0.05 10.4 0.5</size>
          </box>
           </geometry>
        </collision>

        <sensor name='sensor_contact' type='contact'>
          <contact>
            <collision>collision</collision>
            <topic>/world/eyantra_world/time</topic>
          </contact>
          <always_on>1</always_on>
          <update_rate>5</update_rate>
        </sensor>
        
      </link>
    </model>
</sdf>
